@article{islam_integrated_????,
  title = {An integrated model to understand {{Information System}} continuance in an organization},
  abstract = {The success of an information system (IS) depends on the continued usage
(continuance) of the system. In this study, we extend the Expectation-Confirmation Theory (ECT)
based IS continuance theory by including constructs from the Task Technology Fit (TTF) and
Theory of Planned Behavior (TPB) to better understand the IS continuance intention in an
organization. It would help the managers to take decisions on the IS to make it more effective.
Based on a vast literature review, we propose an integrated model that would be an applicable
theoretical lens to explain why users continuously engage with using a system in an organization.
The proposed model consists constructs such as confirmation, perceived usefulness, tasktechnology
fit, perceived playfulness, satisfaction, subjective norm, self efficacy, controllability,
perceived behavioral control, and continuance intention. The proposed model will be used for
further empirical studies in both voluntary and mandatory settings.},
  timestamp = {2015-11-11T20:18:15Z},
  author = {Islam, A. K. M. Najmul},
